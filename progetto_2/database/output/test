DROP TABLE IF EXISTS googleplaystore_processed;
CREATE TABLE googleplaystore_processed(
    Index INT,
    App VARCHAR(256),
    Category VARCHAR(256),
    Rating FLOAT(50),
    Reviews INT,
    Size INT,
    Installs INT,
    Type VARCHAR(15),
    Price FLOAT,
    Content_Rating VARCHAR(30),
    Genres VARCHAR(50),
    Last_Updated TIMESTAMP,
    Age_Restriction INT
);
